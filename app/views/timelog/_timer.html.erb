<% content_for :header_tags do %>
    <% unless current_language.to_s == 'en' %>
        <% if File.exists?(File.join(File.dirname(__FILE__), "../../../assets/javascripts/i18n/#{current_language.to_s}.js")) %>
            <%= javascript_include_tag "i18n/#{current_language.to_s}.js", :plugin => "redmine_timelog_timer" %>
        <% end %>
    <% end %>
<%= javascript_include_tag "store.js", "timer.js", :plugin => "redmine_timelog_timer" %>
<% end %>
